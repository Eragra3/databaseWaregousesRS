  <DataSets>
    <DataSet Name="UserActivityByCountry">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT        COUNT(*) AS ActivityCount, Countries.DistinctCountries
FROM            FactPostAction INNER JOIN
                         UsersNew ON FactPostAction.UserId = UsersNew.Id INNER JOIN
                         Cities ON UsersNew.CityId = Cities.Id INNER JOIN
                         Countries ON Cities.CountryId = Countries.Id
WHERE        (Countries.DistinctCountries IS NOT NULL) AND (Countries.DistinctCountries &lt;&gt; '')
GROUP BY Countries.Id, Countries.DistinctCountries
ORDER BY ActivityCount DESC</CommandText>
      </Query>
      <Fields>
        <Field Name="ActivityCount">
          <DataField>ActivityCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="DistinctCountries">
          <DataField>DistinctCountries</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="UserActivityByCity">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT        COUNT(*) AS ActivityCount, Cities.City
FROM            FactPostAction INNER JOIN
                         UsersNew ON FactPostAction.UserId = UsersNew.Id INNER JOIN
                         Cities ON UsersNew.CityId = Cities.Id
WHERE        (Cities.City IS NOT NULL) AND (Cities.City &lt;&gt; '')
GROUP BY Cities.Id, Cities.City
ORDER BY ActivityCount DESC</CommandText>
      </Query>
      <Fields>
        <Field Name="ActivityCount">
          <DataField>ActivityCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="City">
          <DataField>City</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="ByCountry">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@SelectedCountry">
            <Value>=Parameters!Country.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        COUNT(*) AS ActivityCount, Cities.City
FROM            FactPostAction INNER JOIN
                         UsersNew ON FactPostAction.UserId = UsersNew.Id INNER JOIN
                         Cities ON UsersNew.CityId = Cities.Id INNER JOIN
                         Countries ON Cities.CountryId = Countries.Id
WHERE        (Countries.DistinctCountries = @SelectedCountry)
GROUP BY Cities.City
ORDER BY ActivityCount DESC</CommandText>
      </Query>
      <Fields>
        <Field Name="ActivityCount">
          <DataField>ActivityCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="City">
          <DataField>City</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
